<h1>Projects<small style='padding-left: 10px;'><%= link_to 'New Project', new_project_path %></small></h1>
<br/>
<% @projects.each do |project| %>
  <div class='project span3'>
      <%= link_to project_path(project) do %>
        <%= image_tag project.picture %>
      <% end %><br/>
      <%= link_to project.name, project_path(project) %><br/>
  </div>
<% end %>
<div class='cb'/>
